<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.ifmvo.bases">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:name="cn.ifmvo.App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name="cn.ifmvo.demo.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="cn.ifmvo.demo.TestTopBarBaseActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="cn.ifmvo.demo.TestBottomTabBaseActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="cn.ifmvo.demo.TestRecyclerViewBaseActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>